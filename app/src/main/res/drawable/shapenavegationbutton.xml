<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="oval"  >
            <corners android:radius="10dip"/>
            <stroke android:width="1dip" android:color="#FF8B2145" />
            <gradient android:angle="-90" android:startColor="#FFCF3368" android:endColor="#FFCF3368"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="oval"  >
            <corners android:radius="10dip"/>
            <stroke android:width="1dip" android:color="#FF8B2145" />
            <solid android:color="#FF8B2145"/>
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <corners android:radius="10dip"/>
            <stroke android:width="1dip" android:color="@color/primary_dark" />
            <gradient android:angle="-90" android:startColor="@color/accent" android:endColor="@color/accent" />
        </shape>
    </item>



</selector>